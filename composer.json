{
    "name": "symfony-cmf/content-type",
    "description": "Extends form framework to allow front-end content types",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "symfony/form": "^2.8|^3.0",
        "jms/metadata": "^1.5",
        "symfony/options-resolver": "^2.8|^3.1",
        "sylius/registry": "^0.19.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Cmf\\Component\\ContentType\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\Cmf\\Component\\ContentType\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
